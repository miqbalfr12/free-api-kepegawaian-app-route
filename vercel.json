{
 "headers": [
  {
   "source": "/(.*)",
   "headers": [
    {
     "key": "Cache-Control",
     "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
    },
    {
     "key": "CDN-Cache-Control",
     "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
    },
    {
     "key": "Vercel-CDN-Cache-Control",
     "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
    }
   ]
  }
 ]
}
